{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,ico,gif}", "/css/*", "/js/*", "/*.{css,js,png,jpg,ico,gif}"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate"
  },
  "routes": [
    {
      "route": "/admin/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/start",
      "rewrite": "/index.html"
    },
    {
      "route": "/cardapio",
      "rewrite": "/index.html"
    },
    {
      "route": "/carrinho",
      "rewrite": "/index.html"
    },
    {
      "route": "/encerrada",
      "rewrite": "/index.html"
    }
  ]
}